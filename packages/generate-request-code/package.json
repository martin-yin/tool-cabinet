{
  "name": "generate-request-code",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "dev": "rollup -w -c rollup.config.js",
    "build": "rollup -c rollup.config.js && build-types",
    "build-types": "run-s build-temp-types roll-types",
    "build-temp-types": "tsc -p tsconfig.build.json",
    "roll-types": "api-extractor run && rimraf types",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "generate-request-code": "bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./index.cjs"
    }
  },
  "author": "martin-yin",
  "homepage": "https://github.com/martin-yin/web-dev-tools/tree/main/packages/generate-request-code",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/underscore": "^1.11.4",
    "@types/url-parse": "^1.4.8",
    "axios": "^0.26.1",
    "cac": "^6.7.12",
    "picocolors": "^1.0.0",
    "typedoc": "^0.22.17",
    "underscore": "^1.13.2",
    "url": "^0.11.0",
    "url-parse": "^1.5.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "esbuild": "^0.14.38",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.5.1",
    "rollup": "^2.70.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  }
}
